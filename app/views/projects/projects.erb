
<% if logged_in? %>
  <p>You can edit or delete only your own projects.</p>
  <% @projects.each do |project| %>
    <br></br>
    <a href='/projects/<%=project.id%>'><%= project.name %><%= "  (*your project)" if project.user_id == current_user.id %></a>
  <% end %>

  <br></br>
  <form action='/projects/new' method='get'>
    <input type='submit' value='Create Project'>
  </form>
<% else %>
  <h3>You must be logged in to view projects</h3>
<% end %>
<br></br>
